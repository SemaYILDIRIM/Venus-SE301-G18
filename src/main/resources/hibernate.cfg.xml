<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/se?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">root</property>
    <mapping class="" file="" jar="" package="" resource="proman/Projectuser.hbm.xml"/>
    <mapping class="UserManagement.User" file="" jar="" package="UserManagement" resource="UserManagement/User.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="IssueManagement/Attachment.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="IssueManagement/Comment.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="IssueManagement/Issue.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="IssueManagement/Issuehistory.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="proman/Project.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
